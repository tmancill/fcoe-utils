#!/usr/bin/make -f

#export DH_VERBOSE=1

%:
	dh $@;

override_dh_auto_configure:
	./bootstrap.sh
	./configure --prefix=/usr --sysconfdir=/etc

override_dh_auto_install:
	[ -f etc/initd/fcoe ] || rm -f etc/initd/fcoe
	dh_auto_install

override_dh_installchangelogs:
	dh_installchangelogs debian/changelog
